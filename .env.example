# Supabase (cloud) configuration
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key
SUPABASE_ANON_KEY=your_anon_key

# PGlite (local) configuration
# Note: PGlite will be used if PGLITE_DATA_DIR is set, even if Supabase credentials are present
PGLITE_DATA_DIR=./data/tandem-db
PGLITE_SNAPSHOT_PATH=./data/tandem-db.tar
PGLITE_SNAPSHOT_INTERVAL_MS=3600000
